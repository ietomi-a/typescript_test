{
    "compilerOptions": {
	"outDir": "./out/",
	"sourceMap": true,
	"strictNullChecks": true,
	"strictPropertyInitialization": true,
	"noImplicitAny": false,  // error message が処理し切れないので断念.
	"noImplicitReturns": true,
	"module": "commonjs",
	"target": "es5",
	"jsx": "react"
    },
    "include": [
	"./src/**/*"
    ]
}
